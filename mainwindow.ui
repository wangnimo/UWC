/* 关闭按钮（若启用了 setTabsClosable(true)） */
QTabBar::close-button {
    image: url(:/icons/close.png);
    subcontrol-position: right;
    width: 12px;
    height: 12px;
    margin-left: 4px;
}
QTabBar::close-button:hover {
    image: url(:/icons/close-hover.png);
}

/* ===================== QTabWidget ===================== */
QTabWidget {
    /* 整个控件背景，无特殊需求可透明 */
    background: transparent;
}

/* 页面区域（stacked widget） */
QTabWidget::pane {
    border: 1px solid #374151;   /* 深海灰边框 */
    /* background: #131c29;         比主背景稍亮，突出内容 */
    border-radius: 4px;
    top: -1px;                   /* 让 pane 与 tab 无缝贴合 */
}

/*======== 自定义 ======*/
QPushButton#toggleBtn{
	padding: 0px 0px;
}
QWidget#drawer_widget{
	background-color: rgb(0, 146, 175);
	border: 1px solid #000;
}
/* 消息框整体样式 */
 QMessageBox#msgBox{
 	background-color: #f0f0f0;
  	border: 1px solid #ccc;
	border-radius: 5px;
	height: 100px;
 }

 /* 标题栏样式 */
 QMessageBox#msgBox QLabel[text=&quot;确认操作&quot;] {
	font-size: 16px;
	font-weight: bold;
	color: #333;
	margin: 10px;
 }</string>
